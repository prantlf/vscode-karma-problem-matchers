      "options": {
        "cwd": "test-jasmine"
      },
      "isBackground": true,
      "problemMatcher": {
        "owner": "javascript",
        "severity": "error",
        "fileLocation": [
          "relative",
          "${workspaceFolder}"
        ],
        "pattern": [
          {
            "regexp": "\\) (.+) FAILED",
            "code": 1
          },
          {
            "regexp": "(Expected .*)",
            "message": 1
          },
          {
            "regexp": "<Jasmine>"
          },
          {
            "regexp": "@([^:]+):(\\d+):(\\d+)",
            "file": 1,
            "line": 2,
            "column": 3
          }
        ],
        "background": {
          "activeOnStart": false,
          "beginsPattern": "^\\d+",
          "endsPattern": "^TOTAL:"
        }
      }
    },
